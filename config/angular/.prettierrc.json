{
    "printWidth": 80,
    "tabWidth": 2,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "overrides": [
        {
            "files": [
                "**/*.css",
                "**/*.scss",
                "**/*.html"
            ],
            "options": {
                "singleQuote": false
            }
        }
    ]
}